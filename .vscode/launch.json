{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "SuperMarketApp",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/bin/Debug/net9.0/SuperMarketApp.dll",
        "args": [
            "run",
            "--project",
            "${workspaceFolder}/SuperMarketApp.csproj"
        ],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      }
    ]
  }
  